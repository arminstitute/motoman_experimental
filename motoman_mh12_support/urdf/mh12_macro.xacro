<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_mh12" params="prefix">

  <!-- Properties -->
  <material name="mh12_deep_blue">
        <color rgba="0.12941 0.14902 0.74902 1" />
  </material>

  <!-- Links -->
  <link name="${prefix}base_link">
    <visual>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/visual/base_link.STL" />
      </geometry>
      <material name="mh12_deep_blue"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/collision/base_link.STL" />
      </geometry>
    </collision>
  </link>

  <link name="${prefix}link_s">
    <visual>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/visual/link_s.STL" />
      </geometry>
      <material name="mh12_deep_blue"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/collision/link_s.STL" />
      </geometry>
    </collision>
  </link>
  
  <link name="${prefix}link_l">
    <visual>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/visual/link_l.STL" />
      </geometry>
      <material name="mh12_deep_blue"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/collision/link_l.STL" />
      </geometry>
    </collision>
  </link>

  <link name="${prefix}link_u">
    <visual>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/visual/link_u.STL" />
      </geometry>
      <material name="mh12_deep_blue"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/collision/link_u.STL" />
      </geometry>
    </collision>
  </link>

  <link name="${prefix}link_r">
    <visual>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/visual/link_r.STL" />
      </geometry>
      <material name="mh12_deep_blue"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/collision/link_r.STL" />
      </geometry>
    </collision>
  </link>

  <link name="${prefix}link_b">
    <visual>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/visual/link_b.STL" />
      </geometry>
      <material name="mh12_deep_blue">
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/collision/link_b.STL" />
      </geometry>
    </collision>
  </link>


  <link name="${prefix}link_t">
    <visual>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/visual/link_t.STL" />
      </geometry>
      <material name="mh12_deep_blue">
      </material>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_mh12_support/meshes/collision/link_t.STL" />
      </geometry>
    </collision>
  </link>

  <link name="${prefix}tool0"/>
  
  <!-- JOINTS -->

  <joint name="${prefix}joint_s" type="revolute">
    <origin xyz="0 0 0.45"/>
    <parent link="${prefix}base_link" />
    <child link="${prefix}link_s" />
    <axis xyz="0 0 -1" />
    <limit  lower="-2.9671" upper="2.9671" effort="0" velocity="3.83972" />
  </joint>

  <joint name="${prefix}joint_l" type="revolute">
    <origin xyz="0.155 0 0"/>
    <parent link="${prefix}link_s" />
    <child link="${prefix}link_l" />
    <axis xyz="0 -1 0" />
    <limit lower="-1.5708" upper="2.7053" effort="0" velocity="3.49066" />
  </joint>
 
  <joint name="${prefix}joint_u" type="revolute">
    <origin
      xyz="0 0 0.614"/>
    <parent link="${prefix}link_l" />
    <child link="${prefix}link_u" />
    <axis xyz="0 -1 0" />
    <limit lower="-3.0543" upper="4.1888" effort="0" velocity="3.8397" />
  </joint>
  
  <joint name="${prefix}joint_r" type="revolute">
    <origin xyz="0 0 0.2"/>
    <parent link="${prefix}link_u" />
    <child link="${prefix}link_r" />
    <axis xyz="-1 0 0" />
    <limit lower="-3.1416" upper="3.1416" effort="22" velocity="7.15585" />
  </joint>
  
  <joint name="${prefix}joint_b" type="revolute">
    <origin
      xyz="0.64 0 0"/>
    <parent link="${prefix}link_r" />
    <child link="${prefix}link_b" />
    <axis xyz="0 -1 0" />
    <limit lower="-2.3562" upper="2.3562" effort="22" velocity="7.15585" />
  </joint>

  <joint name="${prefix}joint_t" type="revolute">
    <origin xyz="0.1 0 0"/>
    <parent link="${prefix}link_b" />
    <child link="${prefix}link_t" />
    <axis xyz="-1 0 0" />
    <limit lower="-6.2832" upper="6.2832" effort="9.8" velocity="10.647" />
  </joint>

  <joint name="${prefix}link_t-tool0" type="fixed">
    <origin rpy="0 1.5708 0"/>
    <parent link="${prefix}link_t"/>
    <child link="${prefix}tool0"/>
  </joint>
  
</xacro:macro>
</robot>
